{
  "customErrors": {
    "404": "/404.html"
  },
  "dirListing": false,
  "routes": [
    ["^/post/(?<pid>[^/]*)", "/post/$<pid>.html"]
  ],
  "hooks": {
    "notFound": [["^/post/(?<pid>[^/]*)$", "/post/[pid].js"]]
  },
  "autoExtension": ".html",
  "functions": true,
  "functionsPattern": "^(?!\/_next\/.*).*\\.js$",
  "functionsAutoExtension": ".js"
}
