  },
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/jest": "26.0.23",
    "@types/node": "13.13.4",
    "@types/node-fetch": "2",
    "@vercel/ncc": "0.28.6",
    "eslint": "7.18.0",
    "jest": "27.0.4",
    "prettier": "2.3.1",
    "ts-jest": "27.0.3",
    "typescript": "4.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
